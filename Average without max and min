class Solution(object):
    def average(self, salary):
        """
        :type salary: List[int]
        :rtype: float
        
        """
        mini = min(salary)
        salary.remove(mini)
        maxi = max(salary)
        salary.remove(maxi)

        num = len(salary)
        average = sum(salary) / num
        return average
        
    
        
        
